<?page title="New WMS" contentType="text/html;charset=UTF-8"?>
<?init class="com.trio.util.Interceptors" ?>
<?init class="org.zkoss.zkplus.databind.AnnotateDataBinderInit" ?>
<?link rel="shortcut icon" href="/image/favicon.ico" type="image/x-icon"?>

<zk>
	<window id="win" title="Entry DO Logistik" border="normal"
		width="100%" height="100%"
		apply="com.trio.view.ctrl.EntryDoLogistikCtrl">
		<grid>
			<columns visible="false">
				<column width="100px" />
				<column width="170px" />
				<column width="250px" />
				<column width="100px" />
				<column width="150px" />
				<column width="200px" />
			</columns>
			<rows>
				<row>
					<cell>Kode Dealer</cell>
					<cell>
						<textbox id="tbKodeDealer" />
					</cell>
					<cell>
						<textbox id="tbNamaDealer" cols="30" />
					</cell>
					<cell>
						<button id="btnLovDealer" label="LOV"
							onClick="popDealer.open(self)"
							image="/image/iconcrud/btn_lov.gif" />
					</cell>
					<cell>Tanggal DO Logistik</cell>
					<cell>
						<textbox id="tglDoLogistik" value=""
							disabled="true" width="99%" />
					</cell>
				</row>
				<row>
					<cell>No PO Internal</cell>
					<cell>
						<textbox id="tbNoPoInternal" />
					</cell>
					<cell />
					<cell>
						<button id="btnLovNoPoInternal" label="LOV"
							onClick="popInternal.open(self)"
							image="/image/iconcrud/btn_lov.gif" />
					</cell>
					<cell>No DO Logistik</cell>
					<cell>
						<textbox id="noDoLogistik" value=""
							disabled="true" width="99%" />
					</cell>
				</row>
				<row>
					<cell>Kode Salesman</cell>
					<cell>
						<textbox id="tbKodeSalesman" />
					</cell>
					<cell>
						<textbox id="tbNamaSalesman" cols="30" />
					</cell>
					<cell>
						<button id="btnLovSalesman" label="LOV"
							onClick="popSalesman.open(self)"
							image="/image/iconcrud/btn_lov.gif" />
					</cell>
					<cell />
					<cell />
				</row>
				<row>
					<cell>Lokasi Asal</cell>
					<cell>
						<textbox id="tbLokasi" />
					</cell>
					<cell>
						<textbox id="tbAlamat" cols="30" />
					</cell>
					<cell>
						<button id="btnLovLokasi" label="LOV"
							onClick="popLokasi.open(self)"
							image="/image/iconcrud/btn_lov.gif" />
					</cell>
					<cell />
					<cell />
				</row>
				<row>
					<cell></cell>
				</row>
				<row>
					<cell />
					<cell />
					<cell>Estimasi Open Plafon</cell>
					<cell>
						<textbox id="tbOpenPlafon" disabled="true" />
					</cell>
					<cell />
					<cell />
				</row>
			</rows>
		</grid>
		<separator />
		<grid>
			<columns visible="false">
				<column width="270px" />
				<column width="80px" />
				<column width="100px" />
				<column width="100px" />
				<column width="90px" />
				<column width="100px" />
				<column width="90px" />
			</columns>
			<rows>
				<row>
					<cell />
					<cell>Kode Item</cell>
					<cell>
						<textbox id="kdItem" cols="8" forward="onOK"
							disabled="true" />
					</cell>
					<cell>
						<button id="btnLovItem"
							onClick="popItem.open(self)" label="LOV"
							image="/image/iconcrud/btn_lov.gif" />
					</cell>
					<cell />
					<cell />
					<cell />
				</row>
				<row>
					<cell />
					<cell>
						Qty OH
						<textbox id="tbQtyOh" cols="4" disabled="true" />
					</cell>
					<cell>
						Qty Book
						<textbox id="tbQtyBook" cols="4"
							disabled="true" />
					</cell>
					<cell>
						Qty RFS
						<textbox id="tbQtyRFS" cols="4" disabled="true" />
					</cell>
					<cell>
						Qty PO
						<textbox id="tbQtyPO" cols="4" disabled="true" />
					</cell>
					<cell>
						Qty Sisa
						<textbox id="tbQtySisa" cols="4"
							disabled="true" />
					</cell>
					<cell>
						Qty DO
						<textbox id="tbQtyDO" cols="4" />
					</cell>
					<cell>
						<button id="btnAddDetail" label="Add Detail" />
					</cell>
				</row>
			</rows>
		</grid>

		<space height="20px" />

		<hbox align="start" spacing="10px">
			<listbox id="lbDtldologList" width="400px" height="350px"
				mold="paging" pageSize="10">
				<listhead>
					<listheader width="200px" label="Kode Item"
						sort="auto(kdItem)" />
					<listheader width="200px" label="Qty"
						sort="auto(qty)" />
				</listhead>
			</listbox>

			<vbox>
				<button id="btSimpan" label="Simpan" mold="trendy" width="100px" image="/image/save.png"  />
				<button id="btRefresh" label="Bersihkan" mold="trendy" width="100px" image = "/image/iconcrud/btn_search.gif"/>
			</vbox>
		</hbox>

		<!-- BGN LOV DEALER -->
		<popup id="popDealer" width="425px">
			<vbox>
				<hbox align="center" width="100%">
					Nama Dealer
					<textbox id="popDlrNama" mold="rounded" />
					<button id="popSearch" label="Search" mold="trendy"
						image="/image/search.png" />
					<button id="popReset" label="Reset" mold="trendy"
						image="/image/iconcrud/btn_reset.gif" />
				</hbox>
				<listbox id="popDealerList" width="410px" height="350px"
					mold="paging" pageSize="10">
					<listhead>
						<listheader width="100px" label="Kode Dealer"
							sort="auto(kdDlr)" />
						<listheader width="310px" label="Nama Dealer"
							sort="auto(dlrNama)" />
					</listhead>
				</listbox>
			</vbox>
		</popup>
		<!-- END LOV DEALER -->

		<!-- BGN LOV NO PO INTERNAL -->
		<popup id="popInternal" width="415px">
			<vbox>
				<listbox id="popInternalList" width="400px"
					height="350px" mold="paging" pageSize="10">
					<listhead>
						<listheader width="200px" label="No PO Internal"
							sort="auto(noPoint)" />
						<listheader width="200px" label="Nama Dealer"
							sort="auto(dealerKdDlr.dlrNama)" />
					</listhead>
				</listbox>
			</vbox>
		</popup>
		<!-- END LOV NO PO INTERNAL -->

		<!-- BGN LOV SALESMAN -->
		<popup id="popSalesman" width="415px">
			<vbox>
				<listbox id="popSalesmanList" width="400px"
					height="350px" mold="paging" pageSize="10">
					<listhead>
						<listheader width="200px" label="Kode Sales"
							sort="auto(kodeSales)" />
						<listheader width="200px" label="Nama Sales"
							sort="auto(namaSales)" />
					</listhead>
				</listbox>
			</vbox>
		</popup>
		<!-- END LOV SALESMAN -->

		<!-- BGN LOV LOKASI -->
		<popup id="popLokasi" width="415px">
			<vbox>
				<listbox id="popLokasiList" width="400px" height="350px"
					mold="paging" pageSize="10">
					<listhead>
						<listheader width="200px" label="Lokasi"
							sort="auto(lokasi)" />
					</listhead>
				</listbox>
			</vbox>
		</popup>
		<!-- END LOV LOKASI -->

		<!-- BGN LOV ITEM -->
		<popup id="popItem" width="315px">
			<vbox>
				<hbox align="center" width="100%">
					Kode Item
					<textbox id="popKditem" />
					<button id="btnPopItem" label="Search"
						image="/image/search.png" />
				</hbox>
				<listbox id="popItemList" width="300px" height="350px"
					mold="paging" pageSize="10">
					<listhead>
						<listheader width="100px" label="Kode Item"
							sort="auto(kdItem)" />
						<listheader width="100px" label="Qty On Hand"
							sort="auto(qtyOnhand)" />
						<listheader width="100px" label="Qty Book"
							sort="auto(qtyBook)" />
					</listhead>
				</listbox>
			</vbox>
		</popup>
		<!-- END LOV ITEM -->


	</window>
</zk>